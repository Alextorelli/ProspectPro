{
  "rewrites": [
    { "source": "/", "destination": "/index-supabase.html" },
    { "source": "/app.js", "destination": "/supabase-app.js" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}