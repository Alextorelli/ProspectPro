SUPABASE_SECRET_KEY=sb_secret_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOi...
SUPABASE_ANON_KEY=eyJhbGciOi...
SUPABASE_PUBLISHABLE_KEY=sb_publishable_xxx
SUPABASE_DB_POOLER_URL=postgresql://postgres.<project-ref>:<password>@<region>.pooler.supabase.com:6543/postgres

# REQUIRED API KEYS FOR REAL DATA
GOOGLE_PLACES_API_KEY=your_google_places_api_key
SCRAPINGDOG_API_KEY=your_scrapingdog_api_key
HUNTER_IO_API_KEY=your_hunter_io_api_key
NEVERBOUNCE_API_KEY=your_neverbounce_api_key

# ENHANCED STATE REGISTRY & GOVERNMENT APIS
ZEROBOUNCE_API_KEY=your_zerobounce_api_key
COURTLISTENER_API_KEY=your_courtlistener_api_key
SOCRATA_API_KEY=your_socrata_api_key
SOCRATA_APP_TOKEN=your_socrata_app_token
USPTO_TSDR_API_KEY=your_uspto_api_key

# FOURSQUARE PLACES API (NEW)
FOURSQUARE_CLIENT_ID=your_foursquare_client_id
FOURSQUARE_CLIENT_SECRET=your_foursquare_client_secret

# AUTHENTICATION & SECURITY
PERSONAL_ACCESS_TOKEN=your_personal_access_token_here

# PERFORMANCE & RATE LIMITING
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=15000
REQUEST_DELAY=1000
CACHE_TTL=3600

# COST MANAGEMENT & LIMITS
DAILY_BUDGET_LIMIT=25.00
MONTHLY_BUDGET_LIMIT=150.00
PER_LEAD_COST_LIMIT=2.00
MIN_CONFIDENCE_SCORE=80
MIN_PREVALIDATION_SCORE=70
MAX_LEADS_PER_CAMPAIGN=500

# APPLICATION SETTINGS
NODE_ENV=development
PORT=3000
DEBUG_MODE=true
LOG_LEVEL=info
TRACK_API_COSTS=true
ENABLE_REALTIME=true

# DEPLOYMENT SETTINGS
ALLOWED_ORIGINS=http://localhost:3000,https://your-domain.com
SESSION_TIMEOUT_HOURS=24
MAX_FILE_SIZE_MB=10
UPLOAD_DIR=uploads

# OPTIONAL: THIRD-PARTY INTEGRATIONS
# SENTRY_DSN=https://your-sentry-dsn.ingest.sentry.io
# SENDGRID_API_KEY=SG.your-sendgrid-api-key
# FROM_EMAIL=noreply@yourapp.com
# STRIPE_SECRET_KEY=sk_test_...
# STRIPE_PUBLISHABLE_KEY=pk_test_...
# STRIPE_WEBHOOK_SECRET=whsec_...

# DEVELOPMENT ONLY
SKIP_AUTH_IN_DEV=false
MOCK_API_RESPONSES=false
ENABLE_DEV_ADMIN=true