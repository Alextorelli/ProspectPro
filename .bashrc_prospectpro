# ProspectPro Development Shortcuts

# Add these aliases to your ~/.bashrc or ~/.bash_profile

# ProspectPro shortcuts

alias pp-dev="cd /workspaces/ProspectPro && supabase functions serve"
alias pp-deploy="supabase functions deploy"
alias pp-logs="supabase functions logs --follow"
alias pp-mcp="cd /workspaces/ProspectPro/mcp-servers && npm run start:production"
alias pp-test="curl -X POST 'http://localhost:54321/functions/v1/business-discovery-optimized' -H 'Content-Type: application/json' -d '{\"businessType\":\"test\",\"location\":\"test\"}'"
alias pp-vercel="cd /workspaces/ProspectPro/public && vercel --prod"
alias pp-status="ps aux | grep -E '(production-server|development-server|troubleshooting-server)'"

# Quick navigation

alias pp="cd /workspaces/ProspectPro"
alias ppf="cd /workspaces/ProspectPro/supabase/functions"
alias ppm="cd /workspaces/ProspectPro/mcp-servers"

# Development shortcuts

alias pp-clean="cd /workspaces/ProspectPro && npm run clean"
alias pp-health="npm run health:check"

echo "ProspectPro development aliases loaded!"
echo "Available commands: pp-dev, pp-deploy, pp-logs, pp-mcp, pp-test, pp-vercel, pp-status"
