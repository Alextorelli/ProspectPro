{
  "name": "prospect-pro-real-api",
  "version": "3.0.0",
  "description": "Production-grade lead generation platform with zero-fake-data policy and Supabase Vault integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "production:start": "./scripts/init-prod-server.sh",
    "production:checklist": "./scripts/production-checklist.sh",
    "production:validate-db": "node scripts/validate-production-database-v31.js",
    "prod:init": "./scripts/init-prod-server.sh",
    "prod:setup-env": "./scripts/pull-env-from-secrets.js",
    "prod:check": "./scripts/production-checklist.sh",
    "health": "curl http://localhost:3000/health || echo 'Server not running'",
    "diag": "curl http://localhost:3000/diag | json_pp || echo 'Server not running'",
    "test": "echo 'Tests moved to testing branch. Run: git checkout testing && node tests/validation/test-real-data.js'",
    "postinstall": "echo 'ProspectPro v3.0: Production-ready deployment configured'",
    "docker:dev": "./docker/start-dev.sh",
    "docker:prod": "./docker/deploy-prod.sh",
    "docker:build": "docker-compose build",
    "docker:package": "./docker/create-client-package.sh",
    "docker:logs": "docker-compose logs -f prospectpro",
    "docker:stop": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "secure:setup": "./docker/secure-start.sh setup",
    "secure:start": "./docker/secure-start.sh start",
    "secure:dev": "./docker/secure-start.sh dev",
    "keychain:setup": "./docker/keychain-start.sh setup",
    "keychain:start": "./docker/keychain-start.sh start",
    "1password:setup": "./docker/1password-start.sh setup",
    "1password:start": "./docker/1password-start.sh start",
    "vault:deploy": "echo 'ðŸ” Deploying with Supabase Vault integration...' && docker-compose up --build -d",
    "vault:dev": "echo 'ðŸ” Starting development with Supabase Vault...' && docker-compose -f docker-compose.dev.yml up --build",
    "vault:logs": "docker-compose logs -f prospectpro",
    "vault:test": "echo 'ðŸ§ª Testing Vault connection...' && docker-compose exec prospectpro curl -f http://localhost:3000/diag"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "p-limit": "^3.1.0",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^2.4.2"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supabase": "^2.40.7",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "lead-generation",
    "business-intelligence",
    "api-integration"
  ],
