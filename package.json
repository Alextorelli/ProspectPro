{
  "name": "prospect-pro-real-api",
  "version": "2.0.0",
  "description": "Cost-optimized lead generation platform with real API integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node test/test-real-data.js",
    "test:websites": "node test/test-website-validation.js",
    "test:all": "npm run test && npm run test:websites",
    "test:production": "node scripts/validate-production.js",
    "validate": "npm run test:all && npm run test:production",
    "setup": "cp .env.example .env && echo 'Please edit .env with your API keys'",
    "db:init": "node scripts/initialize-database.js",
    "db:test": "node scripts/initialize-database.js",
    "railway:check": "node debug-railway-imports.js",
    "build": "echo 'No build step required - Node.js project'",
    "postinstall": "echo 'ProspectPro: Zero mock data policy enforced'",
    "security:audit": "node scripts/security-audit.js",
    "env:validate": "node scripts/validate-environment.js",
    "deployment:ready": "node scripts/deployment-readiness.js",
    "railway:deploy": "railway up",
    "railway:logs": "railway logs",
    "admin:dashboard": "echo 'Admin dashboard available at /admin-dashboard.html with PERSONAL_ACCESS_TOKEN'"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "rate-limiter-flexible": "^2.4.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "lead-generation",
    "business-intelligence",
    "api-integration"
  ],
  "author": "ProspectPro Development Team",
  "license": "MIT"
}
