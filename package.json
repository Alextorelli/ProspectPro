{
  "name": "prospect-pro-real-api",
  "version": "2.0.0",
  "description": "Cost-optimized lead generation platform with real API integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "health": "curl http://localhost:3000/health || echo 'Server not running'",
    "test": "echo 'Tests moved to testing branch. Run: git checkout testing && node tests/validation/test-real-data.js'",
    "test:foursquare": "node test/test-foursquare-integration.js",
    "mcp:server": "node tools/mcp/mcp-server.js",
    "docker:build": "docker build -t prospectpro:latest .",
    "docker:run": "docker run --rm -p 3000:3000 --env-file .env -v \"$PWD/uploads:/app/uploads\" --name prospectpro prospectpro:latest",
    "docker:stop": "docker stop prospectpro 2>/dev/null || true",
    "postinstall": "echo 'ProspectPro: Production-ready deployment configured'"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "rate-limiter-flexible": "^2.4.2"
  },
  "devDependencies": {
    "@modelcontextprotocol/sdk": "^1.18.1",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "supabase": "^2.40.7",
    "supertest": "^7.1.4"
  },
  "keywords": [
    "lead-generation",
    "business-intelligence",
    "api-integration"
  ],
  "author": "ProspectPro Development Team",
  "license": "MIT"
}