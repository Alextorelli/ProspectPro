{
  "theme": "dark",
  "themeVariables": {
    "primaryColor": "#222",
    "primaryTextColor": "#f5f5f5",
    "lineColor": "#888",
    "background": "#161616",
    "fontFamily": "Inter, Arial, sans-serif",
    "fontSize": "16px",
    "nodePadding": "12",
    "nodeBorderRadius": "6"
  },
  "flowchart": {
    "defaultRenderer": "elk",
    "elk": {
      "elk.algorithm": "layered",
      "elk.spacing.nodeNode": 40
    }
  },
  "init": {
    "logLevel": "fatal"
  }
}
