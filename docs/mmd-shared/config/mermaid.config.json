{
  "theme": "dark",
  "themeVariables": {
    "primaryColor": "#2b2b2b",
    "primaryTextColor": "#f5f5f5",
    "primaryBorderColor": "#cfcfcf",
    "lineColor": "#b0b0b0",
    "secondaryColor": "#3a3a3a",
    "tertiaryColor": "#1f1f1f",
    "nodeTextColor": "#f5f5f5",
    "mainBkg": "#161616",
    "edgeLabelBackground": "#1a1a1a",
    "clusterBkg": "#1f1f1f",
    "clusterBorder": "#d0d0d0",
    "fontFamily": "Inter, 'Segoe UI', sans-serif"
  },
  "fontFamily": "Inter, 'Segoe UI', sans-serif",
  "flowchart": {
    "useMaxWidth": true,
    "diagramPadding": 20,
    "htmlLabels": false,
    "curve": "basis",
    "padding": 10,
    "defaultRenderer": "elk",
    "elk": {
      "mergeEdges": false,
      "nodePlacementStrategy": "BRANDES_KOEPF",
      "edgeRouting": "POLYLINE",
      "separateConnectedComponents": true,
      "mergeHierarchyEdges": true
    }
  },
  "sequence": {
    "mirrorActors": true,
    "activationMode": "double",
    "wrap": true,
    "width": 240,
    "height": 600,
    "useMaxWidth": true
  },
  "er": {
    "diagramPadding": 30,
    "borderRadius": 4,
    "minEntityWidth": 200
  }
}
