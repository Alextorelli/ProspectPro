{
  "name": "prospectpro-mcp-servers",
  "version": "2.1.0",
  "description": "Consolidated MCP servers for ProspectPro Supabase-First AI development",
  "type": "module",
  "main": "production-server.js",
  "scripts": {
    "start:production": "node production-server.js",
    "start:development": "node development-server.js",
    "start:troubleshooting": "node supabase-troubleshooting-server.js",
    "start:observability": "node observability-server.js",
    "start:all": "concurrently \"npm run start:production\" \"npm run start:development\" \"npm run start:troubleshooting\" \"npm run start:observability\"",
    "test": "node test-servers.js",
    "validate": "npm run test && echo 'âœ… All Supabase MCP servers validated successfully'",
    "install:deps": "npm install",
    "debug:anon-key": "echo 'Use troubleshooting server: diagnose_anon_key_mismatch tool'",
    "debug:edge-functions": "echo 'Use troubleshooting server: test_edge_function tool'",
    "debug:database": "echo 'Use troubleshooting server: validate_database_permissions tool'"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@supabase/supabase-js": "^2.58.0",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/sdk-trace-node": "^0.52.1",
    "@opentelemetry/sdk-trace-base": "^0.52.1",
    "@opentelemetry/exporter-jaeger": "^1.25.1",
    "@opentelemetry/semantic-conventions": "^1.25.1",
    "@opentelemetry/resources": "^1.25.1",
    "p-retry": "^6.2.0",
    "p-timeout": "^6.1.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "prospectpro",
    "supabase",
    "edge-functions",
    "serverless"
  ],
  "author": "Alex Torelli",
  "license": "MIT"
}
