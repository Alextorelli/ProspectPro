{
  "inputs": [
    {
      "id": "memory_file_path",
      "type": "promptString",
      "description": "Path to the memory storage file",
      "password": false
    },
    {
      "id": "browser_url",
      "type": "promptString",
      "description": "Optional: connect to an already-running Chrome (remote debugging / port-forward). Example: http://127.0.0.1:9222",
      "password": false
    },
    {
      "id": "headless",
      "type": "promptString",
      "description": "Run Chrome headless (true/false). Default: false",
      "password": false,
      "default": "false"
    },
    {
      "id": "isolated",
      "type": "promptString",
      "description": "Use a temporary user-data-dir (true/false). Default: false",
      "password": false,
      "default": "false"
    },
    {
      "id": "chrome_channel",
      "type": "promptString",
      "description": "Chrome channel: stable | canary | beta | dev (default: stable).",
      "password": false,
      "default": "stable"
    },
    {
      "id": "context7_api_key",
      "type": "promptString",
      "description": "Context7 API key (optional; increases rate limits). Get one at https://context7.com/dashboard",
      "password": true
    },
    {
      "id": "api_key",
      "type": "promptString",
      "description": "Your API key for the service",
      "password": true
    }
  ],
  "servers": {
    "stripe/agent-toolkit": {
      "type": "http",
      "url": "https://mcp.stripe.com",
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/f0b8ecdd-498c-4087-8ae2-b2677215b7ca",
      "version": "1.0.0"
    },
    "postmanlabs/postman-mcp-server": {
      "type": "stdio",
      "command": "npx",
      "args": ["@postman/postman-mcp-server@latest"],
      "env": {
        "POSTMAN_API_KEY": "${input:api_key}"
      },
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/40516270-789a-bcde-f012-345678901234",
      "version": "1.0.0"
    },
    "apify/apify-mcp-server": {
      "type": "http",
      "url": "https://mcp.apify.com",
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/1aef85cf-1597-47f1-84ae-3e9a771c2162",
      "version": "0.0.1-seed"
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp?project_ref=sriycekxdqnesdsgwiuc&features=docs,account,database,debugging,development,functions,storage,branching"
    },
    "microsoft/playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/41b79849-7e6c-4fc7-82c0-5a611ea21523",
      "version": "0.0.1-seed"
    },
    "github/github-mcp-server": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/ab12cd34-5678-90ef-1234-567890abcdef",
      "version": "0.13.0"
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "${input:context7_api_key}"
      }
    },
    "sequentialthinking": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    }
  }
}
