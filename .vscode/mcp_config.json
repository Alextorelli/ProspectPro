{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"],
      "env": {},
      "metadata": {
        "purpose": "Browser automation, performance profiling, visual regression testing",
        "optimizations": {
          "batch_operations": true,
          "performance_thresholds": {
            "first_contentful_paint": "< 1.5s",
            "largest_contentful_paint": "< 2.5s",
            "cumulative_layout_shift": "< 0.1",
            "first_input_delay": "< 100ms"
          }
        }
      }
    },
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      },
      "metadata": {
        "purpose": "Code repository management, CI/CD integration, issue tracking",
        "optimizations": {
          "pr_automation": true,
          "security_scanning": true,
          "test_orchestration": true
        }
      }
    },
    "supabase-troubleshooting": {
      "command": "node",
      "args": [
        "${workspaceFolder}/dev-tools/agents/mcp-servers/supabase-troubleshooting-server.js"
      ],
      "env": {
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_SERVICE_KEY": "${SUPABASE_SERVICE_KEY}",
        "LOG_RETENTION_HOURS": "168"
      },
      "metadata": {
        "purpose": "Log aggregation, error correlation, real-time monitoring",
        "optimizations": {
          "log_correlation": true,
          "real_time_monitoring": true,
          "intelligent_filtering": true
        }
      }
    },
    "postgresql": {
      "command": "node",
      "args": [
        "${workspaceFolder}/dev-tools/agents/mcp-servers/postgresql-server.js"
      ],
      "env": {
        "DATABASE_URL": "${DATABASE_URL}",
        "QUERY_TIMEOUT_MS": "30000",
        "CONNECTION_POOL_SIZE": "10"
      },
      "metadata": {
        "purpose": "Database operations, query optimization, schema management",
        "optimizations": {
          "query_analysis": true,
          "index_recommendations": true,
          "migration_validation": true
        }
      }
    },
    "integration-hub": {
      "command": "node",
      "args": [
        "${workspaceFolder}/dev-tools/agents/mcp-servers/integration-hub-server.js"
      ],
      "env": {
        "ZAPIER_API_KEY": "${ZAPIER_API_KEY}",
        "STRIPE_SECRET_KEY": "${STRIPE_SECRET_KEY}",
        "SLACK_BOT_TOKEN": "${SLACK_BOT_TOKEN}"
      },
      "metadata": {
        "purpose": "Third-party service integration (Zapier, Stripe, notifications)",
        "optimizations": {
          "webhook_management": true,
          "payload_validation": true,
          "notification_routing": true
        }
      }
    },
    "observability": {
      "command": "node",
      "args": [
        "${workspaceFolder}/dev-tools/agents/mcp-servers/observability-server.js"
      ],
      "env": {
        "JAEGER_ENDPOINT": "http://localhost:14268/api/traces",
        "OTLP_ENDPOINT": "${OTLP_ENDPOINT}",
        "OTLP_AUTH_TOKEN": "${OTLP_AUTH_TOKEN}"
      },
      "metadata": {
        "purpose": "Distributed tracing, performance monitoring, workflow observability",
        "optimizations": {
          "distributed_tracing": true,
          "performance_monitoring": true,
          "error_correlation": true
        }
      }
    },
    "prospectpro-production": {
      "command": "node",
      "args": [
        "${workspaceFolder}/dev-tools/agents/mcp-servers/production-server.js"
      ],
      "env": {},
      "metadata": {
        "purpose": "Production MCP server for ProspectPro",
        "optimizations": {
          "autoStart": true,
          "lazyLoad": true,
          "maxMemory": "256MB",
          "timeout": 30000
        }
      }
    },
    "prospectpro-development": {
      "command": "node",
      "args": [
        "${workspaceFolder}/dev-tools/agents/mcp-servers/development-server.js"
      ],
      "env": {},
      "metadata": {
        "purpose": "Development MCP server for ProspectPro",
        "optimizations": {
          "autoStart": false,
          "lazyLoad": true,
          "maxMemory": "128MB"
        }
      }
    },
    "prospectpro-troubleshooting": {
      "command": "node",
      "args": [
        "${workspaceFolder}/dev-tools/agents/mcp-servers/supabase-troubleshooting-server.js"
      ],
      "env": {},
      "metadata": {
        "purpose": "Troubleshooting MCP server for ProspectPro",
        "optimizations": {
          "autoStart": false,
          "lazyLoad": true,
          "maxMemory": "128MB"
        }
      }
    },
    "stripe-agent-toolkit": {
      "type": "http",
      "url": "https://mcp.stripe.com",
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/f0b8ecdd-498c-4087-8ae2-b2677215b7ca",
      "version": "1.0.0",
      "metadata": {
        "purpose": "Stripe MCP agent toolkit",
        "source": "user-global-config"
      }
    },
    "supabase": {
      "type": "http",
      "url": "https://mcp.supabase.com/mcp?project_ref=sriycekxdqnesdsgwiuc&features=docs%2Caccount%2Cdatabase%2Cdebugging%2Cdevelopment%2Cfunctions%2Cstorage%2Cbranching",
      "metadata": {
        "purpose": "Hosted Supabase MCP server",
        "source": "user-global-config"
      }
    },
    "microsoft-playwright-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/41b79849-7e6c-4fc7-82c0-5a611ea21523",
      "version": "0.0.1-seed",
      "metadata": {
        "purpose": "Playwright MCP server",
        "source": "user-global-config"
      }
    },
    "postmanlabs-postman-mcp-server": {
      "type": "stdio",
      "command": "npx",
      "args": ["@postman/postman-mcp-server@latest"],
      "env": {
        "POSTMAN_API_KEY": "${input:api_key}"
      },
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/40516270-789a-bcde-f012-345678901234",
      "version": "1.0.0",
      "metadata": {
        "purpose": "Postman MCP server",
        "source": "user-global-config"
      }
    },
    "apify-apify-mcp-server": {
      "type": "http",
      "url": "https://mcp.apify.com",
      "gallery": "https://api.mcp.github.com/2025-09-15/v0/servers/1aef85cf-1597-47f1-84ae-3e9a771c2162",
      "version": "0.0.1-seed",
      "metadata": {
        "purpose": "Apify MCP server",
        "source": "user-global-config"
      }
    }
  }
}
