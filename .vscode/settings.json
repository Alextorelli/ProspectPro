{
  "deno.enable": true,
  "deno.enablePaths": ["./app/backend/functions"],
  "deno.lint": true,
  "deno.unstable": ["bare-node-builtins", "byonm", "sloppy-imports"],
  "deno.path": "/home/node/.deno/bin/deno",
  "supabase.projectRef": "sriycekxdqnesdsgwiuc",
  "supabase.workspaceFolder": "${workspaceFolder}",
  "supabase.functions.autoWatch": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "github.copilot.editor.enableCodeActions": true,
  "github.copilot.inlineSuggest.enable": true,
  "github.copilot.chat.codeGeneration.useInstructionFiles": true,
  "github.copilot.chat.agent.runTasks": true,
  "github.copilot.chat.agent.autoFix": false,
  "github.copilot.chat.historyCount": 3,
  "github.copilot.chat.welcomeMessage": "never",
  "github.copilot.chat.localeOverride": "en",
  "github.copilot.chat.dynamicContextTrailingLength": 150,
  "github.copilot.advanced.setAutoCompletionTriggerThreshold": 150,
  "github.copilot.advanced": {
    "debug.overrideEngine": "gpt-4",
    "debug.useNodeFetcher": true
  },
  "chat.agent.maxRequests": 20,
  "chat.editing.autoAcceptDelay": 100,
  "chat.useAgentsMdFile": true,
  "chat.instructionsFilesLocations": {
    ".github": true,
    ".vscode/context": true,
    ".vscode/prompts": true
  },
  "chat.promptFiles": true,
  "chat.promptFilesLocations": {
    ".github/prompts": true,
    ".vscode/prompts": true
  },
  "chat.checkpoints.enabled": true,
  "chat.checkpoints.showFileChanges": true,
  "vitest.commandLine": "npm run test:devtools",
  "vitest.configFile": "app/frontend/vite.config.devtools.ts",
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.suggest.autoImports": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "search.searchOnType": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.minimap.enabled": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "files.associations": {
    "**/app/backend/functions/**/*.ts": "typescript",
    "**/app/backend/functions/**/index.ts": "typescript",
    "supabase/migrations/*.sql": "sql",
    "*.sql": "sql",
    "*.mdx": "markdown",
    ".env*": "dotenv",
    "mcp-config.json": "jsonc",
    "copilot-instructions.md": "markdown",
    "*.mmd": "mermaid"
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js, ${capture}.d.ts, ${capture}.test.ts",
    "*.tsx": "${capture}.test.tsx",
    "package.json": "package-lock.json, tsconfig.json, vercel.json",
    ".env": ".env.*, .env.local, .env.example, .env.vercel"
  },
  "files.watcherExclude": {
    "**/supabase/.temp/**": true,
    "**/node_modules/**": true,
    "**/dev-tools/agents/mcp-servers/node_modules/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.deno_lsp/**": true,
    "**/dist/**": true,
    "**/archive/**": true,
    "**/logs/**": true,
    "**/.vscode/extensions/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true,
    "**/.git": true,
    "**/supabase/.temp": true,
    "**/archive": true,
    "**/dev-tools/agents/mcp-servers/logs": true
  },
  "ai.contextFiles": [
    "app/backend/functions/*/index.ts",
    "docs/app/CODEBASE_INDEX.md",
    "src/lib/supabase.ts",
    "TECHNICAL_SUMMARY_v4.3.md",
    "PROSPECTPRO_V4.3_RELEASE_NOTES.md",
    ".vscode/settings.json"
  ],
  "ai.excludeFiles": [
    "archive/**",
    "docs/**",
    "dev-tools/agents/mcp-servers/node_modules/**",
    ".deno_lsp/**",
    "*.log"
  ],
  "git.ignoreLimitWarning": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.fetchOnPull": true,
  "git.mergeEditor": false,
  "mcp.enable": true,
  "mcp.servers": {
    "prospectpro-production": {
      "command": "node",
      "args": ["./dev-tools/agents/mcp-servers/observability-server.js"],
      "autoStart": true,
      "lazyLoad": true,
      "maxMemory": "256MB",
      "timeout": 30000
    },
    "prospectpro-development": {
      "command": "node",
      "args": ["./dev-tools/agents/mcp-servers/observability-server.js"],
      "autoStart": false,
      "lazyLoad": true,
      "maxMemory": "128MB"
    },
    "prospectpro-troubleshooting": {
      "command": "node",
      "args": [
        "./dev-tools/agents/mcp-servers/supabase-troubleshooting-server.js"
      ],
      "autoStart": false,
      "lazyLoad": true,
      "maxMemory": "128MB"
    }
  },
  "terminal.integrated.cwd": "${workspaceFolder}",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "terminal.integrated.env.linux": {
    "SUPABASE_PROJECT_REF": "sriycekxdqnesdsgwiuc"
  },
  "terminal.integrated.profiles.linux": {
    "supabase": {
      "path": "/bin/bash",
      "args": ["-c", "cd ${workspaceFolder}/supabase && exec bash"]
    }
  },
  "tasks.showQuickPick": true,
  "chat.todoListTool.writeOnly": true,
  "chat.agent.todoList": {
    "position": "default"
  },
  "sqltools.connections": [
    {
      "name": "Supabase Local",
      "driver": "PostgreSQL",
      "previewLimit": 50,
      "server": "127.0.0.1",
      "port": 54322,
      "database": "postgres",
      "username": "postgres",
      "password": "postgres"
    }
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[sql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "on"
  },
  "chat.edits2.enabled": true,
  "chat.todoListTool.descriptionField": true,
  "chat.emptyState.history.enabled": true,
  "chat.mcp.gallery.enabled": true,
  "chat.mcp.discovery.enabled": {
    "windsurf": true
  },
  "chat.tools.terminal.autoReplyToPrompts": true,
  "chat.useNestedAgentsMdFiles": true,
  "mermaidChart.enableLivePreview": true,
  "mermaidChart.exportOnSave": false,
  "mermaidChart.defaultTheme": "dark",
  "mermaidChart.externalSnippets": ["docs/shared/mermaid/config/mermaid.json"],
  "vitest.nodeEnv": {}
}
