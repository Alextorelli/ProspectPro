{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Test Business Discovery Optimized",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/supabase/functions/business-discovery-optimized/index.ts",
      "env": {
        "SUPABASE_URL": "https://sriycekxdqnesdsgwiuc.supabase.co",
        "SUPABASE_ANON_KEY": "${env:SUPABASE_ANON_KEY}"
      },
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Test Campaign Export",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/supabase/functions/campaign-export/index.ts",
      "env": {
        "SUPABASE_URL": "https://sriycekxdqnesdsgwiuc.supabase.co"
      },
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Debug MCP Production Server v4.1",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/mcp-servers/production-server.js",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Debug MCP Development Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/mcp-servers/development-server.js",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Debug MCP Troubleshooting Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/mcp-servers/troubleshooting-server.js",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Debug Current File",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "env": {
        "NODE_ENV": "development"
      },
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "console": "integratedTerminal"
    }
  ],
  "compounds": [
    {
      "name": "Debug Edge Functions",
      "configurations": [
        "Test Business Discovery Optimized",
        "Test Campaign Export"
      ]
    }
  ]
}
